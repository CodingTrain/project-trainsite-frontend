{
    "extends": [
      "tslint-config-airbnb",
      "tslint-react"
    ],
    "rules": {
      "import-name": false,
      "jsx-ban-props": [
        true,
        ["className", "Prefer styled-components"],
        ["id", "Prefer styled-components."]
      ],
      "variable-name": [true, "allow-pascal-case"]
    }
}
